unit orm.lib.Helper.RttiHelper;

interface

uses System.Rtti;

type
  TRttiFieldHelper = class Helper for TRttiField
    private
    public
      function Tem<T: TCustomAttribute>: Boolean;
  end;


implementation

{ TRttiFieldHelper }

function TRttiFieldHelper.Tem<T>: Boolean;
begin
  Result := GetAtribute<T> <> nil;
end;

end.
